{
  "name": "gamelife",
  "version": "2.0.1",
  "versionName": "2.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "build": "cd android && ./gradlew bundleRelease",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "install-android": "npm run prestart && cd android && ./gradlew installRelease && cd .. && npm run poststart",
    "install-server": "cd src/Server/TCP && npm install",
    "clean": "cd android && ./gradlew clean",
    "test": "jest --detectOpenHandles",
    "lint": "eslint .",
    "server": "cd src/Server/TCP && npm start",
    "podi": "cd ios && pod install",
    "prestart": "npx tsc -b src/Types/tsconfig.gamelife.json",
    "poststart": "find src/Types/ -type f \\( -name \"*.js\" -o -name \"*.d.ts\" -o -name \"*.tsbuildinfo\" \\) -delete",
    "apple-reset-cache": "rm -rf ~/Library/Developer/Xcode/DerivedData/GameLife-*/Build/Intermediates.noindex/XCBuildData/"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/blur": "^4.4.0",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "crypto-js": "^4.2.0",
    "jest-raw-loader": "^1.0.1",
    "react": "^18.3.1",
    "react-native": "0.77.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.13.2",
    "react-native-exit-app": "^1.1.0",
    "react-native-gifted-charts": "1.4.29",
    "react-native-google-mobile-ads": "^14.8.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-permissions": "^5.2.4",
    "react-native-safe-area": "^0.5.1",
    "react-native-shadow-2": "^7.0.6",
    "react-native-svg": "^15.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/codegen": "^0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/gradle-plugin": "^0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/websocket": "^1.0.10",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ft-flow": "^3.0.10",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.6.3",
    "obfuscator-io-metro-plugin": "^2.0.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.3.1",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
