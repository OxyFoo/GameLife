{
  "name": "gamelife",
  "version": "1.0.0",
  "versionName": "1.0.0",
  "private": true,

  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",

    "build-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./ios/gamelife.jsbundle --assets-dest ./ios",
    "build-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "test": "jest",
    "lint": "eslint ."
  },

  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/datetimepicker": "^6.1.3",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-firebase/admob": "11.5.0",
    "@react-native-firebase/app": "11.5.0",
    "react": "^18.1.0",
    "react-native": "^0.68.0",
    "react-native-codegen": "^0.0.16",
    "react-native-device-info": "^8.7.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal-datetime-picker": "^13.1.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-shadow-2": "^6.0.5",
    "react-native-svg": "^12.3.0",
    "react-native-tracking-transparency": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "obfuscator-io-metro-plugin": "^2.0.0",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
