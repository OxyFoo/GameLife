{
  "name": "gamelife",
  "version": "2.0.2",
  "versionName": "2.0.2",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "build": "cd android && ./gradlew bundleRelease",
    "test": "jest --detectOpenHandles",
    "lint": "eslint ./src",
    "podi": "cd ios ; pod deintegrate && rm -rf Podfile.lock Pods/ && pod install",
    "android": "npx react-native run-android",
    "android:release": "cd android && ./gradlew installRelease",
    "ios": "npx react-native run-ios",
    "clean": "npm run clean:gradle && npm run clean:pod && npm run clean:android && npm run clean:ios",
    "clean:gradle": "cd android && rm -rf app/.cxx && ./gradlew --stop && ./gradlew clean && cd ..",
    "clean:pod": "if [ \"$(uname)\" != \"Darwin\" ]; then echo 'Not macOS, aborting pod install'; else cd ios && pod deintegrate && rm -rf build Pods Podfile.lock && cd ..; fi",
    "clean:android": "rm -rf android/.gradle android/build android/app/build",
    "clean:ios": "rm -rf ios/build ~/Library/Developer/Xcode/DerivedData/GameLife-*/Build/Intermediates.noindex/XCBuildData/"
  },
  "dependencies": {
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-google-signin/google-signin": "^14.0.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "crypto-js": "^4.2.0",
    "jest-raw-loader": "^1.0.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-app-control": "^1.0.1",
    "react-native-config": "^1.5.5",
    "react-native-device-info": "^14.0.4",
    "react-native-gifted-charts": "1.4.61",
    "react-native-google-mobile-ads": "^15.2.0",
    "react-native-google-play-integrity": "^1.1.0",
    "react-native-iap": "^12.16.2",
    "react-native-ios-appattest": "^1.0.1",
    "react-native-keychain": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-permissions": "^5.4.0",
    "react-native-pinned-ws": "^1.0.2",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area": "^0.5.1",
    "react-native-shadow-2": "^7.1.1",
    "react-native-svg": "^15.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.6",
    "@oxyfoo/gamelife-types": "^2.0.7",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/codegen": "0.79.2",
    "@react-native/eslint-config": "^0.79.2",
    "@react-native/gradle-plugin": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.4",
    "@types/react-test-renderer": "^19.0.0",
    "@types/websocket": "^1.0.10",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "obfuscator-io-metro-plugin": "^2.1.3",
    "prettier": "^3.5.3",
    "react-test-renderer": "19.0.0",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  }
}
