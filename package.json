{
  "name": "gamelife",
  "version": "1.0.0",
  "versionName": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "build-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./ios/gamelife.jsbundle --assets-dest ./ios",
    "build-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/datetimepicker": "6.5.2",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-firebase/admob": "11.5.0",
    "@react-native-firebase/app": "11.5.0",
    "react": "18.2.0",
    "react-native": "0.71.0",
    "react-native-codegen": "^0.71.3",
    "react-native-device-info": "^10.3.0",
    "react-native-exit-app": "^1.1.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modal-datetime-picker": "^14.0.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-shadow-2": "^7.0.6",
    "react-native-svg": "^12.1.0",
    "react-native-tracking-transparency": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.20.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "metro-react-native-babel-preset": "^0.74.0",
    "obfuscator-io-metro-plugin": "^2.1.1",
    "react-test-renderer": "18.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
